# First open chrome
import pyautogui as pg
import time

time.sleep(3)

pg.hotkey("ctrl" , "t")
time.sleep(1)
pg.typewrite("linkedin")
time.sleep(1)
pg.press("del")
time.sleep(1)
pg.press("enter")
time.sleep(2)

pg.hotkey("ctrl" , "f")
time.sleep(1)
pg.typewrite("linkedin.com")
time.sleep(1)
pg.press("esc")
time.sleep(1)
pg.press("enter")
time.sleep(5)

pg.press("tab")
pg.press("tab")
pg.press("tab")
pg.press("tab")
pg.press("tab")
pg.press("tab")
time.sleep(2)

pg.typewrite("IT company near me")
pg.press("enter")


