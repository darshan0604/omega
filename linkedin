# Find the search bar by its class name and click to focus on it
search_bar_position = pg.locateCenterOnScreen("search_bar.png", confidence=0.9)  # Replace "search_bar.png" with the image file of the search bar
pg.click(search_bar_position)

# Typewrite "vishal" into the search bar
pg.typewrite("vishal")
